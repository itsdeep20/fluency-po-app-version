{
  "appId": "com.fluency.ai.pro",
  "appName": "Fluency AI Pro",
  "webDir": "dist",
  "server": {
    "androidScheme": "https",
    "allowNavigation": [
      "*.firebaseapp.com",
      "*.googleapis.com",
      "*.cloudfunctions.net",
      "*.firebase.com",
      "*.google.com"
    ]
  },
  "plugins": {
    "CapacitorHttp": {
      "enabled": false
    },
    "GoogleAuth": {
      "scopes": [
        "profile",
        "email"
      ],
      "serverClientId": "YOUR_WEB_CLIENT_ID_FROM_FIREBASE",
      "forceCodeForRefreshToken": true
    }
  }
}